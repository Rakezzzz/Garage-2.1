@using Garage_2._1.Models
@model IEnumerable<Garage_2._1.Models.Parkingspot>

@{
    ViewBag.Title = "Index";
}


<h2>Index</h2>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()



    <div style="margin-top:50px;">
        <p>Date: <input type="text" id="datepicker"></p>
    </div>
    //array av enum vehicletype

    <div class="form-inline">
        <h4>
            Filter
        </h4>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })

        <div class="form-group">
            <p>Vehicle Type</p>
        </div>

        <div class="form-group">
            <p>Color</p>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Filter" class="btn btn-default" />
            </div>
        </div>
    </div>
    <hr />
}

@Html.ActionLink("SuperIndex", "SuperIndex") |
@Html.ActionLink("Add Parkingspot to garage", "CreateParkingspot") |
@Html.ActionLink("Add new vehicle", "AddVehicle")



@Html.Partial("~/Views/Shared/_ListParkingspots.cshtml", Model)

