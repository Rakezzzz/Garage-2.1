@model IEnumerable<Garage_2._1.Models.Parkingspot>

@{
    ViewBag.Title = "Index";
}


<h2>Index</h2>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()



    
    //array av enum vehicletype
    
    <div class="form-inline">
        <hr />

        <div class="form-group">
            <p>Vehicle Type</p>
            <input type="checkbox" name="types" value="Car" checked="checked"/> Car
            <input type="checkbox" name="types" value="Motorcycle" checked="checked" /> Motorcycle
            <input type="checkbox" name="types" value="Bus" checked="checked" /> Bus
            <input type="checkbox" name="types" value="Trailer" checked="checked" /> Trailer
        </div>
        <div>
            <p>From: <input name ="dateFrom" type="text" id="datepicker"></p>
            <p>To: <input name="dateTo" type="text" id="datepicker"></p>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Filter" class="btn btn-default" />
            </div>
        </div>
    </div>
    <hr />
}

@Html.ActionLink("SuperIndex", "SuperIndex") |
@Html.ActionLink("Add Parkingspot to garage", "CreateParkingspot") |
@Html.ActionLink("Add new vehicle", "AddVehicle")

@Html.Partial("~/Views/Shared/_ListParkingspots.cshtml", Model)
